{
   "$schema":"http://json-schema.org/draft-07/schema#",
   "definitions":{
      "string":{
               "type":"object",
               "properties":{
                  "is_binary":{
                     "type":"boolean"
                  },
                  "length":{
                     "type":"integer"
                  },
                  "alphabet":{
                     "type":"string"
                  },
                  "min_length":{
                     "type":"integer"
                  },
                  "max_length":{
                     "type":"integer"
                  },
                  "encoding":{
                     "type":"string"
                  },
                  "is_multiple_values":{
                     "type":"boolean"
                  },
                  "enums": {
                     "type": "array"
                  },
                  "restrictions": {
                     "type": "array"
                  }
               },
               "additionalProperties":false,
               "required":[
                  "is_binary"
               ]
            },
      "number":{
               "type":"object",
               "properties":{
                  "is_int":{
                     "type":"boolean"
                  },
                  "length":{
                     "type":"integer"
                  },
                  "lower_bound":{
                     "type":"number"
                  },
                  "upper_bound":{
                     "type":"number"
                  }
               },
               "additionalProperties":false,
               "required":[
                  "is_int"
               ]
            },
      "boolean":{
               "type":"object",
               "additionalProperties":false
            },
      "date":{
        "type":"object",
        "properties":{
          "is_full_date":{
                     "type":"boolean"
                  },
          "past_date":{
                     "type":"boolean"
                  },
          "future_date":{
                     "type":"boolean"
                  },
          "years_or_more":{
                     "type":"integer"
                  },
          "years_or_less":{
                     "type":"integer"
                  }
        },
        "additionalProperties":false
      }
   }
}